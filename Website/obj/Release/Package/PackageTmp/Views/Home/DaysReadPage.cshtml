@model Website.Models.IndexViewModel
@{
    ViewBag.Title = "DaysReadPage";
}

<h2>DaysReadPage</h2>


       <div style="width:100%">
       <div style="clear:left; width:40%; float:left">
        <ul style="list-style: none; font-size:1.4em;">
            @foreach(var item in Model.myList)
            {
               <li> @item.userName</li> 
               
            }
           </ul>
           </div>

        <div style="width:40%; float:left">
        <ul style="list-style:none; font-size:1.4em;">
            @foreach (var item in Model.myList)
            {
               
               <li> @item.daysRead</li>
                
            }
           </ul>
           </div>

           <div style="width:20%; float:left">
        <ul style="list-style: none; font-size:1.4em;">
            @foreach (var item in Model.myList)
            {              
                <li> @item.date</li>
            }
           </ul>
           </div>
           </div>